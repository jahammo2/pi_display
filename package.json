  "license": "MIT",
  "private": false,
  "scripts": {
    "api:test": "npm run db:drop:test && NODE_ENV=test mocha --opts 'support/mocha.opts' 'src/api/**/*.spec.js' --exit",
    "api:start": "nodemon --watch src/api src/api/server.js",
    "build": "webpack -d --progress --colors",
    "build:production": "NODE_ENV=production webpack --progress --colors",
    "client:start": "NODE_ENV=development webpack-dev-server",
    "client:test": "NODE_ENV=test mocha --require babel-core/register --opts 'support/mocha.opts' 'src/client/**/*.spec.js' --exit",
    "db:drop": "node src/api/database/drop.js",
    "db:drop:test": "NODE_ENV=test npm run db:drop",
    "lint": "npm run lint:css && npm run lint:js",
    "lint:css": "stylelint 'src/css/**/*.css' || true",
    "lint:js": "eslint --format 'node_modules/eslint-friendly-formatter' --ext .js,.jsx ./src",
    "start": "npm run api:start & npm run client:start",
    "test": "npm run api:test && npm run client:test"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "es6-promise": "^4.2.4",
    "express": "^4.16.3",
    "history": "^4.7.2",
    "immutable": "^3.8.2",
    "keymirror": "^0.1.1",
    "lodash": "^4.17.10",
    "mongoose": "^5.2.5",
    "normalize.css": "^8.0.0",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-immutable": "^4.0.0",
    "redux-persist": "^5.10.0",
    "redux-persist-cookie-storage": "^1.0.0",
    "redux-persist-immutable": "^4.3.1",
    "redux-thunk": "^2.3.0",
    "sanitize.css": "^6.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
