    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-react": "^7.10.0",
    "faker": "^4.1.0",
    "html-webpack-plugin": "^3.2.0",
    "mocha": "^5.2.0",
    "morgan": "^1.9.0",
    "nodeman": "^1.1.2",
    "nodemon": "^1.18.3",
    "pathfinder-ui": "^1.0.3",
    "postcss-apply": "^0.10.0",
    "postcss-assets": "^5.0.0",
    "postcss-cssnext": "^3.1.0",
